"nanobench.skl": {
    "tool": "nanobench", "version": "master", "uarch": "SKL",
    "unsupported_insns_path": "./filters/error_schemes_{KEY}.csv",
    "config": {
            "kind": "nanobench",
            "nanobench_path": "${BASE_DIR}/predictors/nanobench/repo/nanoBench.sh",
            "nanobench_opts": [
                "-config", "${BASE}/configs/cfg_Skylake_common.txt",
                "-unroll", "20",
                "-warm_up_count", "10",
                "-loop", "100"
            ],
            "timeout": 1.0,
            "num_samples": 3
        }
},


"ithemal.bhive.skl": {
    "tool": "ithemal", "version": "bhive", "uarch": "SKL",
    "unsupported_insns_path": "./filters/error_schemes_{KEY}.csv",
    "config": {
            "kind": "ithemal_docker",
            "host": "127.0.0.1",
            "port": "42010",
            "ssl_path": "${BASE_DIR}/predictors/ithemal/ssl",
            "model": "bhive/skl",
            "timeout": 10.0
        }
},
"ithemal.bhive.hsw": {
